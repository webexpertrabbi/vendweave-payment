{
  "name": "vendweave/payment",
  "version": "1.9.1",
  "description": "Production-grade Laravel payment gateway SDK for VendWeave POS - Auto-adapts to your system",
  "type": "library",
  "keywords": [
    "laravel",
    "payment",
    "gateway",
    "vendweave",
    "pos",
    "bkash",
    "nagad",
    "rocket",
    "upay"
  ],
  "license": "MIT",
  "authors": [
    {
      "name": "VendWeave",
      "email": "dev@vendweave.com"
    }
  ],
  "require": {
    "php": "^8.1",
    "illuminate/support": "^10.0|^11.0",
    "illuminate/http": "^10.0|^11.0",
    "guzzlehttp/guzzle": "^7.0"
  },
  "require-dev": {
    "orchestra/testbench": "^8.0|^9.0",
    "phpunit/phpunit": "^10.0"
  },
  "autoload": {
    "psr-4": {
      "VendWeave\\Gateway\\": "src/"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "VendWeave\\Gateway\\Tests\\": "tests/"
    }
  },
  "extra": {
    "laravel": {
      "providers": [
        "VendWeave\\Gateway\\VendWeaveServiceProvider"
      ],
      "aliases": {
        "VendWeave": "VendWeave\\Gateway\\Facades\\VendWeave"
      }
    }
  },
  "config": {
    "sort-packages": true
  },
  "minimum-stability": "stable",
  "prefer-stable": true
}
